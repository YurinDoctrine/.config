ShowDotFiles = 1
TabTimeout = 0

Terminal = kitty
TermExec = ${Terminal}
AlwaysInTerm = ssh ftp git less more vim cat nano ranger tail top info xsel

History = 5

URL_http = x-www-browser %u
URL_info = ${TermExec} 'info %s'
URL_file = exo-open %s
URL_f = exo-open %s
URL_g = x-www-browser 'http://www.google.com/search?q=%s'
