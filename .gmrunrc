ShowDotFiles = 1
TabTimeout = 0

Terminal = kitty
TermExec = ${Terminal}
AlwaysInTerm = ssh ftp git less more vi vim cat nano ranger tail top info xsel xterm

History = 5

URL_http = xdg-open %u
URL_info = ${TermExec} 'info %s'
