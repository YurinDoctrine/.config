ShowDotFiles = 1
TabTimeout = 0

Terminal = x-terminal-emulator
TermExec = ${Terminal} -e
AlwaysInTerm = ssh ftp lynx mc vi vim cat nano top sudo info

History = 500

URL_http = x-www-browser %u
URL_info = ${TermExec} 'info %s'
URL_file = exo-open %s
URL_f = exo-open %s
URL_g = x-www-browser 'http://www.google.com/search?q=%s'
